SUPPORTED_PLATFORMS = macosx iphoneos iphonesimulator
ARCHS[sdk=macosx*] = i386 x86_64
ARCHS[sdk=iphoneos*] = armv6 armv7
ARCHS[sdk=iphonesimulator*] = i386
VALID_ARCHS[sdk=iphoneos*] = armv6 armv7
VALID_ARCHS[sdk=iphonesimulator*] = i386
DEBUG_INFORMATION_FORMAT = dwarf-with-dsym
GCC_VERSION = 
GCC_VERSION[arch=x86_64] = com.apple.compilers.llvm.clang.1_0
DEPLOYMENT_POSTPROCESSING = YES
MACOSX_DEPLOYMENT_TARGET = 10.4
MACOSX_DEPLOYMENT_TARGET[arch=x86_64] = 10.6
GCC_MODEL_TUNING = G5
OTHER_CFLAGS = -DSTANDALONE_MAC_VERSION=1
GCC_PREFIX_HEADER = MarkdownMacPrefix.h
GCC_PRECOMPILE_PREFIX_HEADER = YES
